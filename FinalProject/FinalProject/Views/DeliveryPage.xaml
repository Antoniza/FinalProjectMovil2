<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FinalProject.Views.DeliveryPage"
             NavigationPage.HasNavigationBar="False">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" Orientation="Vertical">
            <Label Text="Plantilla de Repartidor" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand"/>
            <Label x:Name="UserName" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand"/>
            <Button x:Name="Logout" Text="Logout" BackgroundColor="Black" TextColor="White" Command="{Binding LogOutCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>